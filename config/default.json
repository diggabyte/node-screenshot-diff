{
  "webshot": {
    "screenSize": {
      "width": 1400
    },
    "shotSize": {
      "height": "all"
    },
    "phantomConfig": {
      "ignore-ssl-errors": true
    },
    "customCSS": "",
    "renderDelay": 0
  },
  "compare": {
    "dissimilarityThreshold": 10,
    "fuzz": 5,
    "quality": 100,
    "metric": "AE",
    "highlightColor": "blue"
  },
  "domains": [
    "site.example.com",
    "example.com"
  ],
  "paths": {
    "home":       "/"
  },
  "outputDir": "./screenshots",
  "keepSourceScreenshots": false
}
